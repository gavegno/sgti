-- Database: sgti

-- DROP DATABASE sgti;

CREATE DATABASE sgti
  WITH OWNER = root
       ENCODING = 'UTF8'
       TABLESPACE = pg_default
       LC_COLLATE = 'en_US.UTF-8'
       LC_CTYPE = 'en_US.UTF-8'
       CONNECTION LIMIT = -1;

-- Table: tipohora

-- DROP TABLE tipohora;

CREATE TABLE tipohora
(
  id serial NOT NULL,
  tipo character varying NOT NULL,
  CONSTRAINT "id_primaryKey" PRIMARY KEY (id )
)
WITH (
  OIDS=FALSE
);
ALTER TABLE tipohora
  OWNER TO root;




